    "notificationsInfoIcon.foreground": "#fafafa",

    "banner.background": "#171717",
    "banner.foreground": "#fafafa",
    "banner.iconForeground": "#fafafa",

    /*
     * -----------------------------------------------------------------
     * Panel
     * -----------------------------------------------------------------
     */
    "panel.background": "#0c0c0c",
    "panel.border": "#ffffff1a",
    "panel.dropBorder": "#ffffff1a",
    "panelTitle.activeBorder": "#e5e5e5",
    "panelTitle.activeForeground": "#fafafa",
    "panelTitle.inactiveForeground": "#fafafa80",
    "panelInput.border": "#ffffff1a",

    /*
     * -----------------------------------------------------------------
     * Terminal
     * -----------------------------------------------------------------
     */
    "terminal.background": "#0c0c0c",
    "terminal.foreground": "#d9d9d9",
    "terminalCursor.foreground": "#19aad8",
    "terminalCursor.background": "#0D0D0D",
    "terminal.selectionBackground": "#2f4364",
    "terminal.ansiGreen": "#90c85b",
    "terminal.ansiBrightGreen": "#90c85b",
    "terminal.ansiBlue": "#8eb7db",
    "terminal.ansiBrightBlue": "#8eb7db",
    "terminal.ansiRed": "#d66d5f",
    "terminal.ansiBrightRed": "#d66d5f",
    "terminal.ansiYellow": "#dedda9",
    "terminal.ansiBrightYellow": "#dedda9",
    "terminal.ansiCyan": "#8eb7db",
    "terminal.ansiBrightCyan": "#8eb7db",
    "terminal.ansiMagenta": "#d66d5f",
    "terminal.ansiBrightMagenta": "#d66d5f",
    "terminal.ansiBrightWhite": "#d9d9d9",
    "terminal.ansiBrightBlack": "#0D0D0D",
    "terminal.ansiWhite": "#d9d9d9",
    "terminal.ansiBlack": "#0D0D0D"
  },

  /*
   * ===================================================================
   * TOKEN COLORS (Syntax Highlighting)
   * ===================================================================
   */

  "tokenColors": [
    {
      "scope": [
        "meta.embedded.line.tsx",
        "meta.embedded.line.jsx",
        "string.unquoted.plain.in.js.jsx",
        "string.unquoted.plain.out.js.jsx",
        "meta.jsx.children",
        "meta.tag.children"
      ],
      "settings": {
        "foreground": "#cdd6f4"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7f849c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "scope": ["variable.parameter", "variable.other", "meta.parameter"],
      "settings": {
        "foreground": "#a6adc8"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string", "string.regexp"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "number",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.struct",
        "entity.name.enum"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": ["entity.name.tag", "tag", "meta.tag"],
      "settings": {
        "foreground": "#f38ba8"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "meta.attribute"],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "scope": [
        "support.type.property-name",
        "meta.property-name",
        "variable.object.property",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#cdd6f4"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#89b4fa",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#cdd6f4"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.scope"],
      "settings": {
        "foreground": "#b4befe"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#cba6f7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.variable", "support.other"],
      "settings": {
        "foreground": "#89dceb"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.block.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "scope": ["constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "scope": ["storage.type.primitive", "support.type.primitive"],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "scope": ["variable.language.self", "variable.parameter.self"],
      "settings": {
        "foreground": "#cba6f7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": ".env variable keys (left-hand side)",
      "scope": ["variable.key.dotenv", "source.dotenv"],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "name": ".env variable",
      "scope": ["property.value.dotenv", "source.dotenv"],
      "settings": {
        "foreground": "#a6e3a1"
      }
    }
  ]
}
